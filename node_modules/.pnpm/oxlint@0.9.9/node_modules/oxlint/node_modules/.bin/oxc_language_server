#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/oxlint@0.9.9/node_modules/oxlint/bin/node_modules:/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/oxlint@0.9.9/node_modules/oxlint/node_modules:/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/oxlint@0.9.9/node_modules:/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/oxlint@0.9.9/node_modules/oxlint/bin/node_modules:/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/oxlint@0.9.9/node_modules/oxlint/node_modules:/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/oxlint@0.9.9/node_modules:/home/jdorozco/Work/back/calendar-app-back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/oxc_language_server" "$@"
else
  exec node  "$basedir/../../bin/oxc_language_server" "$@"
fi
